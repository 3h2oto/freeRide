/**app.wxss**/
page{background-color:#f1f1f1;}
.wbox {display: -webkit-flex;display: -webkit-box;display: box;display: flex;}
.wbox-flex {-webkit-flex: 1!important;-webkit-box-flex: 1;}
.bdb{position:relative;}
.bdb:after{position: absolute;right: 0;bottom: 0;left:0;height: 1px;content: '';-webkit-transform: scaleY(.5);transform: scaleY(.5);background-color: #ddd;}
.bdt{position:relative;}
.bdt:before{position: absolute;right: 0;top: 0;left:0;height: 1px!important;content: '';-webkit-transform: scaleY(.5);transform: scaleY(.5);background-color: #ddd;}

@font-face {
  font-family: 'iconfont';  /* project id 1545413 */
  src: url('//at.alicdn.com/t/font_1545413_rlcqnj58fn.eot');
  src: url('//at.alicdn.com/t/font_1545413_rlcqnj58fn.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1545413_rlcqnj58fn.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1545413_rlcqnj58fn.woff') format('woff'),
  url('//at.alicdn.com/t/font_1545413_rlcqnj58fn.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1545413_rlcqnj58fn.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-arrow:before {
  content: "\e60f";
}
